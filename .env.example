# Environment Variables Template
# Copy this file to .env and customize for your environment

# Application Configuration
TZ=Europe/Copenhagen

# Database Configuration
POSTGRES_USER=ats_user
POSTGRES_PASSWORD=ats_password
POSTGRES_DB=ats

# Worker Configuration
ATS_TOKEN=your-api-token-here  # Leave blank for development
ATS_URL=http://api:8000        # Can be left like this if using docker-compose
ATS_CAPABILITIES=playwright    # Additional capabilities can be specified here

# Production Configuration (production only)
# HTTP_PORT=80
# HTTPS_PORT=443
# SSL_CERT_PATH=./ssl

# Development Configuration
# Set to true for development builds
# LOCAL_DEVELOPMENT=true